eView/components/foodCarousel\";\nimport allFoodList from \"@/views/HomeView/allFoodList\";\nimport {fetchList} from \"@/api/dish\";\nimport TextBox from \"@/components/Suggest/TextBox\";\n\n\nexport default {\n  name: \"HomeView\",\n  components: {TextBox, foodCard, foodCarousel, allFoodList},\n  data() {\n    return {\n      suggestDialog:false,\n      items:[],\n      page: 1,\n      itemsPerPage: 6,\n      valid: false,\n      firstname: '',\n      lastname: '',\n      nameRules: [\n        v => !!v || 'Name is required',\n        v => v.length <= 10 || 'Name must be less than 10 characters',\n      ],\n      email: '',\n      emailRules: [\n        v => !!v || 'E-mail is required',\n        v => /.+@.+/.test(v) || 'E-mail must be valid',\n      ]\n    }\n  },\n  created() {\n    this.getList()\n  },\n  computed: {\n    numberOfPages() {\n      return Math.ceil(this.items.length / this.itemsPerPage)\n    }\n  },\n  methods:{\n    showSuggestDialog(){\n      this.suggestDialog = true\n    },\n    getList(){\n      let Did = {\n        \"star_count\":10\n      }\n      fetchList(Did).then(response => {\n        this.list_length = response.data.list_length\n        //console.log(this.list_length)\n        this.items = response.data.dish_list\n        //console.log(this.items)\n      })\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n\n</style>\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgFA,OAAAA,QAAA;AACA,OAAAC,YAAA;AACA,OAAAC,WAAA;AACA,SAAAC,SAAA;AACA,OAAAC,OAAA;AAGA;EACAC,IAAA;EACAC,UAAA;IAAAF,OAAA,EAAAA,OAAA;IAAAJ,QAAA,EAAAA,QAAA;IAAAC,YAAA,EAAAA,YAAA;IAAAC,WAAA,EAAAA;EAAA;EACAK,IAAA,WAAAA,KAAA;IACA;MACAC,aAAA;MACAC,KAAA;MACAC,IAAA;MACAC,YAAA;MACAC,KAAA;MACAC,SAAA;MACAC,QAAA;MACAC,SAAA,GACA,UAAAC,CAAA;QAAA,SAAAA,CAAA;MAAA,GACA,UAAAA,CAAA;QAAA,OAAAA,CAAA,CAAAC,MAAA;MAAA,EACA;MACAC,KAAA;MACAC,UAAA,GACA,UAAAH,CAAA;QAAA,SAAAA,CAAA;MAAA,GACA,UAAAA,CAAA;QAAA,eAAAI,IAAA,CAAAJ,CAAA;MAAA;IAEA;EACA;EACAK,OA