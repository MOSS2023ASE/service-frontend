\n              <v-list-item-title v-html=\"item.user_name\"></v-list-item-title>\n              <v-list-item-subtitle v-html=\"item.suggest_text\"></v-list-item-subtitle>\n            </v-list-item-content>\n          </v-list-item>\n        </template>\n      </v-list>\n    </v-app>\n  </div>\n\n</template>\n\n<script>\nimport {fetchSuggest} from \"@/api/suggest\";\nexport default {\n  name: \"suggest\",\n  data() {\n    return {\n      items: []\n    }\n  },\n  created() {\n    this.getList()\n  },\n  methods:{\n    getList() {\n      fetchSuggest().then(response => {\n        this.list_length = response.data.list_length\n        console.log(this.list_length)\n        let originData = response.data.suggest_list\n        console.log(originData)\n        let divide = { divider: true, inset: true }\n        let o\n        this.$nextTick(() => {\n          this.items = []\n          for(o in originData){\n            this.items.push(originData[o])\n            this.items.push(divide)\n          }\n          console.log(this.items)\n          setTimeout(() => {\n            this.listLoading = false\n          }, 1.5 * 1000)\n        })\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>\n"]}]}